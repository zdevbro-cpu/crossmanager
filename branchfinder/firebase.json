{
  "functions": {
    "source": "functions"
  },
  "hosting": {
    "site": "lasbranch",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/naver/**",
        "function": "naverProxy"
      },
      {
        "source": "/api/naver-search/**",
        "function": "naverSearchProxy"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
